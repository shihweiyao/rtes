<%= form_for(@asset_list) do |f| %>
  <% if @asset_list.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@asset_list.errors.count, "error") %> prohibited this asset_list from being saved:</h2>

      <ul>
      <% @asset_list.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :location %><br />
    <%= f.text_field :location %>
  </div>
  <div class="field">
    <%= f.label :time %><br />
    <%= f.date_select :time , discard_month:true%>
  </div>
  <div class="field">
    <%= f.label :owner %><br />
    <%= f.select :member_id, Member.all.collect {|p| [ p.name, p.id ] } %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
